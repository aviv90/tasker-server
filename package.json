    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/uuid": "^11.0.0",
    "@types/winston": "^2.4.4",
    "axios": "^1.13.1",
    "cloudconvert": "^3.0.0",
