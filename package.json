    "express-rate-limit": "^8.2.1",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "form-data": "^4.0.4",
    "googleapis": "^167.0.0",
    "helmet": "^8.1.0",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "openai": "^6.10.0",
